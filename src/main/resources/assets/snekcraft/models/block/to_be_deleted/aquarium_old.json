{
  "multipart": [

    {   "when": { "down": "0" },
      "apply": { "model": "snekcraft:block/rat_cage_bottom", "uvlock": false }
    },
    {   "when": { "east": "0" },
      "apply": { "model": "snekcraft:block/aquarium", "y" : 90, "uvlock": false }
    },
    {   "when": { "west": "0" },
      "apply": { "model": "snekcraft:block/aquarium", "y" : 270, "uvlock": false }
    },
    {   "when": { "north": "0" },
      "apply": { "model": "snekcraft:block/aquarium", "y" : 0, "uvlock": false }
    },
    {   "when": { "south": "0" },
      "apply": { "model": "snekcraft:block/aquarium", "y" : 180, "uvlock": false }
    },

    {   "when": {
      "AND": [
        {"OR": [{"down": "0"}, {"down": "2"}]},
        {"OR": [{"east": "0"}, {"east": "2"}]}
      ]},
      "apply": { "model": "snekcraft:block/container_base_right", "y" : 0, "uvlock": false }
    },
    {   "when": {
      "AND": [
        {"OR": [{"down": "0"}, {"down": "2"}]},
        {"OR": [{"west": "0"}, {"west": "2"}]}
      ]},      "apply": { "model": "snekcraft:block/container_base_left", "y" : 0, "uvlock": false }
    },
    {   "when": {
      "AND": [
        {"OR": [{"down": "0"}, {"down": "2"}]},
        {"OR": [{"north": "0"}, {"north": "2"}]}
      ]},
      "apply": { "model": "snekcraft:block/rat_cage_bottom_left", "y" : 90, "uvlock": false }
    },
    {  "when": {
      "AND": [
        {"OR": [{"down": "0"}, {"down": "2"}]},
        {"OR": [{"south": "0"}, {"south": "2"}]}
      ]},
      "apply": { "model": "snekcraft:block/rat_cage_bottom_right", "y" : 90, "uvlock": false }
    }
  ]
}